h3 Status

input(type='text',ng-model='query',placeholder='Search...')

table
  tr
    th Driver
    th Origin
    th Name
    th Title
    th Value
    th Unit
    th Time
  tr.repeat-item(ng-repeat='row in status | filter:query | orderBy:"key"')
    td {{row.type}}
    td {{row.tag}}
    td {{row.name}}
    td {{lookup(row).title}}
    td(align='right') {{niceValue(row)}}
    td {{lookup(row).unit}}
    td {{row.time | date:"MM-dd &nbsp; HH:mm:ss"}}
